{
  "compilerOptions": {
    "target": "es6",
    "outDir": "../../lib/server",
    "rootDir": "./",
    "moduleResolution": "node",
    "module": "commonjs",
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "declaration": true,
    "importHelpers": true,
    "inlineSourceMap": true,
    "listFiles": false,
    "traceResolution": false,
    "pretty": true,
    "lib": [
      "es6",
      "dom"
    ],
    "baseUrl": "../.."
  },
  "include": [
    "./**/*.ts",
    "../../node_modules/@types/node/index.d.ts"
  ],
  "exclude": [
    "node_modules/**"
  ],
  "compileOnSave": false
}
